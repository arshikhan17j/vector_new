msg_str = to_string(.message) ?? ""

parsed = parse_regex!(
  msg_str,
  r'type=(?P<type>\S+) msg=audit\((?P<epoch>\d+\.\d+):(?P<event_id>\d+)\): pid=(?P<pid>\d+) uid=(?P<uid>\d+) auid=(?P<auid>\d+) ses=(?P<ses>\d+) subj=(?P<subj>\S+) msg=\'unit=(?P<msg_unit>\S+) comm=(?P<msg_comm>\S+) exe=(?P<msg_exe>\S+) hostname=(?P<msg_hostname>\S+) addr=(?P<msg_addr>\S+) terminal=(?P<msg_terminal>\S+) res=(?P<msg_res>\w+)'
)

if parsed != null {
  .audit_type = parsed.type
  .audit_epoch = parsed.epoch
  .audit_event_id = parsed.event_id
  .pid = parsed.pid
  .uid = parsed.uid
  .auid = parsed.auid
  .session = parsed.ses
  .selinux_subj = parsed.subj
  .UID = parsed.UID
  .AUID = parsed.AUID
  .msg_unit = parsed.msg_unit
  .msg_comm = parsed.msg_comm
  .msg_exe = parsed.msg_exe
  .msg_hostname = parsed.msg_hostname
  .msg_addr = parsed.msg_addr
  .msg_terminal = parsed.msg_terminal
  .msg_res = parsed.msg_res
  
}

.vendor = "RedHat"
.vendor_product = "RedHat Enterprise Linux"
.data_type = .vendor_product + " audit"
.ingestion_time = now()
.raw_event = encode_json(.)
